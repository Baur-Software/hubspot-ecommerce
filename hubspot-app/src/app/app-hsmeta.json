{
  "uid": "app-hubspot-ecommerce-app",
  "type": "app",
  "config": {
    "description": "Complete ecommerce integration for WordPress - sync products, subscriptions, and orders between HubSpot and WordPress.",
    "name": "Baur Software HubSpot Ecommerce for WordPress",
    "distribution": "marketplace",
    "auth": {
      "type": "oauth",
      "redirectUrls": [
        "http://localhost:3000/callback",
        "https://hubspot-shop.local/wp-admin/admin.php?page=hubspot-ecommerce-oauth-callback",
        "https://baur-software.local/wp-admin/admin.php?page=hubspot-ecommerce-oauth-callback",
        "https://baursoftware.com/wp-admin/admin.php?page=hubspot-ecommerce-oauth-callback"
      ],
      "requiredScopes": [
        "crm.objects.contacts.read",
        "crm.objects.contacts.write",
        "crm.objects.deals.read",
        "crm.objects.deals.write",
        "crm.objects.line_items.read",
        "crm.objects.line_items.write",
        "crm.schemas.line_items.read",
        "e-commerce",
        "oauth"
      ],
      "optionalScopes": [],
      "conditionallyRequiredScopes": []
    },
    "permittedUrls": {
      "fetch": [
        "https://api.hubapi.com"
      ],
      "iframe": [],
      "img": []
    },
    "support": {
      "supportEmail": "support@baursoftware.com",
      "documentationUrl": "https://baursoftware.com/hubspot-ecommerce/docs",
      "supportUrl": "https://baursoftware.com/support",
      "supportPhone": "+16195499524"
    }
  }
}